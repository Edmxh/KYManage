<?xml version="1.0" encoding="utf-8"?>
<com.example.kymanage.definedClass.CheckableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/parent_layout"
    android:descendantFocusability="blocksDescendants"
    android:background="@drawable/tablebody4"
    android:showDividers="middle"
    android:divider="@drawable/linearlayout_divider"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="序                 号:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xh"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="序号"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <TextView
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:text="物   料  编   码:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/wlbm"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="物料编码"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="物   料  描   述:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/wlms"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="物料描述"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="生 产 订 单 号:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/scddh"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="生产订单号"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="发   料  反   馈:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/flfk"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="发料反馈"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:showDividers="middle"
        android:divider="@drawable/linearlayout_divider">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xqsl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="发料数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/flsl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="发料数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="单位"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/dw"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="单位"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="状态:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/zt"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="状态"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
    </LinearLayout>
</com.example.kymanage.definedClass.CheckableLayout>