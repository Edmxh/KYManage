<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/parent_layout"
    android:descendantFocusability="blocksDescendants"
    android:background="@drawable/tablebody4"
    android:showDividers="none"
    android:divider="@drawable/linearlayout_divider"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="序                   号:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/xh"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="match_parent"
                    android:text="采购订单号/行:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/cgddh_hang"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="销售订单号/行:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/xsddh_hang"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="物   料   编   码:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/wlbm"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="物   料   描   述:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/wlms"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="物   料   类   型:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/wllx"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="需   求   日   期:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <TextView
                    android:id="@+id/xqrq"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center_vertical"
                    android:duplicateParentState="true"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_width="@dimen/titlewidth2"
                    android:layout_height="wrap_content"
                    android:text="异   常  原   因:"
                    android:textSize="@dimen/text2"
                    android:textColor="@color/fontcolor1"
                    android:gravity="center"
                    android:duplicateParentState="true"/>
                <Spinner
                    android:id="@+id/spinner1"
                    android:layout_width="0dp"
                    android:layout_weight="3"
                    android:textSize="@dimen/text2"
                    android:background="@drawable/spinnerbox"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            >
            <Button
                android:id="@+id/receive"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="收货"
                android:textSize="@dimen/text22"
                android:textColor="@color/colorWhite"
                android:background="@drawable/buttonstyle3"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:showDividers="middle"
        android:divider="@drawable/linearlayout_divider">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xqsl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text=""
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="已入库数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/yrksl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text=""
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="到货数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <EditText
                android:id="@+id/dhsl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text=""
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:background="@color/colorBackground5"
                android:digits="@string/filter_vcode"
                style="@style/EditTextStyle"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>