<?xml version="1.0" encoding="utf-8"?>
<com.example.kymanage.definedClass.CheckableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/parent_layout"
    android:descendantFocusability="blocksDescendants"
    android:background="@drawable/tablebody4"
    android:showDividers="middle"
    android:divider="@drawable/linearlayout_divider"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="序                 号:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xh"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="序号"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="物   料   编   码:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/wlbm"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="物料编码"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="物   料   凭   证:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/wlpz"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="物料凭证"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="过   账   日   期:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/gzrq"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="过账日期"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="凭   证   日   期:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/pzrq"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="凭证日期"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="生 产 订 单  号:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/scddh"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="生产订单号"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="销售订单号/行:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xsddh_hang"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="销售订单号/行"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="供   货   工   厂:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/ghgc"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="供货工厂"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="供   货   库   房:"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/ghkf"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="供货库房"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center_vertical"
                android:duplicateParentState="true"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:showDividers="middle"
        android:divider="@drawable/linearlayout_divider">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求工厂"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xqgc"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求工厂"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求库房"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/xqkf"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="需求库房"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="转储数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/zcsl"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="转储数量"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="状态"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
            <TextView
                android:id="@+id/zt"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="状态"
                android:textSize="@dimen/text2"
                android:textColor="@color/fontcolor1"
                android:textCursorDrawable="@null"
                android:gravity="center"
                android:duplicateParentState="true"/>
        </LinearLayout>
    </LinearLayout>
</com.example.kymanage.definedClass.CheckableLayout>